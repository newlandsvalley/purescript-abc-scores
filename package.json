{
  "private": true,
  "scripts": {
    "pulp-build": "pulp build",
    "pulp-test": "pulp test",
    "build": "spago build",
    "test": "spago -x ./test.dhall test",
    "display-tests": "spago -x ./display-tests.dhall bundle-app --main Examples.DisplayTests.Main --to examples/display-tests/dist/display-tests.js",
    "full-tune": "spago -x ./full-tune.dhall bundle-app --main Examples.FullTune.Main --to examples/full-tune/dist/full-tune.js",
    "align-staves": "spago -x ./align-staves.dhall bundle-app --main Examples.StaveAlignment.Main --to examples/align-staves/dist/align-staves.js",
    "beaming": "spago -x ./beaming.dhall bundle-app --main Examples.Beaming.Main --to examples/beaming/dist/beaming.js",
    "thumbnail": "spago -x ./thumbnail.dhall bundle-app --main Examples.Thumbnail.Main --to examples/thumbnail/dist/thumbnail.js",
    "slurs": "spago -x ./slurs.dhall bundle-app --main Examples.Slurs.Main --to examples/slurs/dist/slurs.js",
    "chord-symbols": "spago -x ./chord-symbols.dhall bundle-app --main Examples.ChordSymbols.Main --to examples/chord-symbols/dist/chord-symbols.js",
    "clefs": "spago -x ./clefs.dhall bundle-app --main Examples.Clefs.Main --to examples/clefs/dist/clefs.js",
    "bugs": "spago -x ./bugs.dhall bundle-app --main Examples.Bugs.Main --to examples/bugs/dist/bugs.js",
    "debug": "spago -x ./debug.dhall run --main Examples.Debug.Main"
  },
  "devDependencies": {
    "pulp": "^15.0.0",
    "purescript": "^0.14.3",
    "spago": "0.20.3",
    "webpack": "^3.6.0"
  }
}
